{
    "compilerOptions": {
        "composite": false,
        "declaration": true,
        "declarationMap": true,
        "esModuleInterop": true,
        "resolveJsonModule": true,
		
        "forceConsistentCasingInFileNames": true,
        "jsx": "react-jsx",
        "inlineSources": false,
        "isolatedModules": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "preserveWatchOutput": true,
        "skipLibCheck": true,
        "strict": true,
    }
}
